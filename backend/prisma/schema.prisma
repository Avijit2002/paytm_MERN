generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  firstname String?
  lastname  String?
  password  String
  updatedAt DateTime @updatedAt
  username  String?  @unique
}
